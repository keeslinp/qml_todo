{
    "cppFile": "src/Bindings.cpp",
    "rust": {
        "dir": "rust",
        "interfaceModule": "interface",
        "implementationModule": "implementation"
    },
    "objects": {
        "Todo": {
            "type": "Object",
            "properties": {
                "tasks": {
                    "type": "Tasks"
                },
                "val": {
                    "type": "QString",
                    "write": true
                }
            }
        },
        "Tasks": {
            "type": "List",
            "itemProperties": {
                "title": {
                    "type": "QString",
                    "write": true,
                    "roles": [ [ "display", "edit" ] ]
                },
                "completed": {
                    "type": "bool",
                    "write": true
                }
            }
        }
    }
}
